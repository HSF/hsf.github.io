{
  "ignorePatterns": [
    {
      "pattern": "/assets/CWP-Charge-HSF.pdf"
    },
    {
      "pattern": "mailto:.*"
    },
    {
      "pattern": "https://twitter.com/PyHEPConf"
    },
    {
      "pattern": "https://ariostas.com"
    },
    {
      "pattern": "https://indico.desy.de"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/(.*)",
      "replacement": "/_site/$1"
    },
    {
      "pattern": "^\\s*{{\\s*site.baseurl\\s*}}\\s*/(.*)",
      "replacement": "/_site/$1"
    }
  ],
  "projectBaseUrl": "/__w/hsf.github.io/",
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
